{
  "compilerOptions": {
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "module": "Preserve",
    "target": "ESNext",
    "moduleResolution": "bundler",
    "moduleDetection": "force",
    "allowImportingTsExtensions": true,
    "verbatimModuleSyntax": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "$api": ["./.buncf/api-client.ts"],
      "$routes": ["./.buncf/routes.ts"]
    }
  },
"types": ["./.buncf/cloudflare-env.d.ts", "./buncf-env.d.ts"],
"include": ["src/**/*", "*.d.ts", ".buncf/routes.ts", ".buncf/cloudflare-env.d.ts"]
}
